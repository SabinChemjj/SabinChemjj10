#include <stdio.h>

int main() 
{
    int num1, num2, num3, num4, num5;
    float average;
    int sum;

    printf("Enter five integers:\n");
    scanf("%d", &num1);
    scanf("%d", &num2);
    scanf("%d", &num3);
    scanf("%d", &num4);
    scanf("%d", &num5);

    sum = num1 + num2 + num3 + num4 + num5;
    average = (float)sum / 5;

    printf("The average of the five integers is: %.2f\n", average);

    return 0;
}
